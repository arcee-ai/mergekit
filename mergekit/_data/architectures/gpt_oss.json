            {
                "name": "model.layers.${layer_index}.mlp.experts.gate_up_proj_bias",
                "output_space": "up_${layer_index}",
                "is_bias": true
            },
            {
                "name": "model.layers.${layer_index}.mlp.experts.down_proj",
                "input_space": "up_${layer_index}",
                "output_space": "running_residual"
            },
            {
                "name": "model.layers.${layer_index}.mlp.experts.down_proj_bias",
                "input_space": "running_residual",
                "is_bias": true
            },
            {
                "name": "model.layers.${layer_index}.mlp.router.weight",
                "input_space": "running_residual"
            },
            {
                "name": "model.layers.${layer_index}.mlp.router.bias",
                "is_bias": true
            }
        ]
